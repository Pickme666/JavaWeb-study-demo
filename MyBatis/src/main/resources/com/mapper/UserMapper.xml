<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--
    namespace 命名空间
    resultType 结果集返回类型
    id 语句唯一标识
-->
<mapper namespace="com.mapper.UserMapper">
    <!--Statement-->
    <select id="selectAll" resultType="com.pojo.User">
        select * from user;
    </select>
</mapper>